                                                    disable-search="false"
                                                    ng-model="selectedLanguage"
                                                    ng-init="selectedLanguage = null"
                                                    ng-options="language.code as language.languageName for language in languagesList track by language.code">
                                                <option value="" disabled selected>Select a language</option>
                                            </select>
                                        </div>
                                        <div class="single-element-error">
                                            <p class="error-label"><span>Select a language</span></p>
                                        </div>
                                    </div>
                                    <div class="button-group-container">
